<script src="https://checkout.razorpay.com/v1/checkout.js"></script>
<script>

  $(document).ready(function () {
    $('.pay-form').submit(function (e) {
      e.preventDefault();

      var formData = $(this).serialize();

      $.ajax({
        url: "/createOrder",
        type: "POST",
        data: formData,
        success: function (res) {
          if (res.success) {
            var options = {
              "key": "" + res.key_id + "",
              "amount": "" + res.amount + "",
              "currency": "INR",
              "name": "" + res.product_name + "",
              "description": "" + res.description + "",
              "image": "https://st.depositphotos.com/1435425/4077/v/950/depositphotos_40771829-stock-illustration-indian-rupee-sign-golden-coin.jpg",
              "order_id": "" + res.order_id + "",
              "handler": function (response) {
                alert("Payment Succeeded");
                // window.open("/","_self")
              },
              "prefill": {
                "contact": "" + res.contact + "",
                "name": "" + res.name + "",
                "email": "" + res.email + ""
              },
              "notes": {
                "description": "" + res.description + ""
              },
              "theme": {
                "color": "#2300a3"
              }
            };
            var razorpayObject = new Razorpay(options);
            razorpayObject.on('payment.failed', function (response) {
              alert("Payment Failed");
            });
            razorpayObject.open();
          }
          else {
            alert(res.msg);
          }
        }
      })

    });
  });

</script>